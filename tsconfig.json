{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "sourceMap": false,
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "declaration": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "types": [
      "node", "mocha"
    ],
    "lib": [
      "dom",
      "es2020"
    ]
  },
  "exclude": [
    "test",
    "node_modules",
    "**/*.d.ts"
  ]
}
